<a  name="services"></a>
<% services.forEach(function(item) { %>
<div class="<% if (item.position == 0) { %> content-section-a <% } else { %> content-section-b <% } %>">
    <div class="container">
        <div class="row">
            <div class="<% if (item.position == 0) { %> col-lg-5 col-sm-6 <% } else { %> col-lg-5 col-lg-offset-1 col-sm-push-6  col-sm-6 <% } %>">
                <hr class="section-heading-spacer">
                <div class="clearfix"></div>
                <h2 class="section-heading"><%= item.title %></h2>
                <p class="lead"><%= item.description %></p>
            </div>
            <div class="<% if (item.position == 0) { %> col-lg-5 col-lg-offset-2 col-sm-6 <% } else { %> col-lg-5 col-sm-pull-6  col-sm-6 <% } %>">
                <img class="img-responsive" src="img/<%= item.artwork %>" alt="">
            </div>
        </div>
    </div>
    <!-- /.container -->
</div>
<%  }); %>
<!-- /.content-section-X -->
